{
  "extends": ["eslint:recommended", "plugin:react/recommended"],
  "env": { "browser": true, "node": true },
  "parserOptions": {
    "ecmaVersion": 10,
    "sourceType": "module",
    "ecmaFeatures": {
      "jsx": true
    }
  },
  "rules": {
    "react/jsx-filename-extension": "off"
  }
}
